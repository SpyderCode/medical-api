            "datasource": {
              "type": "prometheus",
              "uid": "prometheus"
            },
            "expr": "sum(rate(container_cpu_usage_seconds_total{pod=~\"medical-api-.*\"}[1m])) by (pod) / sum(container_spec_cpu_quota{pod=~\"medical-api-.*\"} / 100000) by (pod) * 100",
